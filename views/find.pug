extends layout

block content
    h1=title
    style.
        #map {
            width: 100%;
            height: 500px;}

    br
    #map

    script.
        var map;
        var flushes = [];
    each location, i in locations
        script.
            flushes.push(
            {
                location: '#{location.name}',
                latitude: '#{location.latitude}',
                longitude: '#{location.longitude}',
                id: '#{location.id}',
            }, )       

    script.
        function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: { lat: 39.9507456, lng: -75.60232959999999 },
            zoom: 17
        });
        
        

        flushes.forEach(function(flush) {
            var marker = new google.maps.Marker({
            position: new google.maps.LatLng(flush.latitude, flush.longitude),
            icon: {
                url: 'data:image/png;base64,',
                scaledSize: new google.maps.Size(47, 53)
            },
            map: map,
            title: flush.location,
            url: '/location/'+flush.id,
            animation: google.maps.Animation.DROP
            });
            google.maps.event.addListener(marker, 'click', function() {
            window.location.href = this.url;
            });
        });
        var marker = new google.maps.Marker({
        position: new google.maps.LatLng(39.9507456, -75.60232959999999),
        icon: {
            url: 'data:image/png;base64,',
            scaledSize: new google.maps.Size(55, 55)
        },
        map: map,
        title: 'Current Location',
        animation: google.maps.Animation.DROP
        });
        }

    script(async defer src=`https://maps.googleapis.com/maps/api/js?key=${google_map_api_key}&callback=initMap`)
